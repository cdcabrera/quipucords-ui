{
  "name": "quipucords-ui",
  "version": "2.0.0",
  "description": "Quipucords UI",
  "author": "Red Hat",
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/quipucords/quipucords-ui.git"
  },
  "bugs": {
    "url": "https://github.com/quipucords/quipucords-ui/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "export NODE_ENV=production; run-s -l build:js test:integration",
    "build:js": "weldable -l ts -x ./config/webpack.prod.config.js",
    "start": "export NODE_ENV=development; run-p -l start:js",
    "start:js": "weldable -l ts -x ./config/webpack.dev.config.js",
    "test": "export NODE_ENV=test; run-s test:types test:lint test:ci",
    "test:ci": "export CI=true; jest ./src --coverage",
    "test:clearCache": "jest --clearCache",
    "test:dev": "export NODE_ENV=test; run-s test:lint test:local",
    "test:lint": "eslint --ext=json --ext=ts --ext=tsx ./src",
    "test:lint-fix": "npm run test:lint -- --fix",
    "test:local": "jest --roots=./src --watch",
    "test:integration": "jest ./tests",
    "test:integration-dev": "jest --roots=./tests --watch",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@mturley-latest/react-table-batteries": "^1.0.4",
    "@patternfly/patternfly": "^5.1.0",
    "@patternfly/react-core": "^5.0.0",
    "@patternfly/react-icons": "^5.0.0",
    "@patternfly/react-styles": "^5.0.0",
    "@patternfly/react-table": "^5.1.1",
    "@tanstack/react-query": "^5.8.4",
    "axios": "^1.7.2",
    "detect-browser": "^5.3.0",
    "i18next": "^23.4.4",
    "i18next-xhr-backend": "^2.0.1",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^13.1.2",
    "react-router-dom": "6.24.1"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.51.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "7.14.1",
    "@typescript-eslint/parser": "7.14.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-comment-length": "^1.7.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jsdoc": "^48.7.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-watch-typeahead": "2.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "ts-jest": "^29.2.2",
    "weldable": "^3.1.0"
  }
}
